{
  "parser": "babel-eslint",
  "extends" : [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "react/forbid-prop-types": 0,
    "no-param-reassign": 0,
    "no-class-assign": 0,
    "react/jsx-wrap-multilines": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-shadow": 0,
    "default-case": 0,
    "implicit-arrow-linebreak": 0
  }
}
